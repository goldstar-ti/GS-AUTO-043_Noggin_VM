138: def calculate_md5_hash(file_path: Path) -> str:
139:     """Calculate MD5 hash of file"""
140:     try:
141:         hash_md5 = hashlib.md5()
142:         with open(file_path, 'rb') as f:
143:             for chunk in iter(lambda: f.read(8192), b''):
144:                 hash_md5.update(chunk)
145:         return hash_md5.hexdigest()
