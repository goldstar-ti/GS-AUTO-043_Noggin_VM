 664: class CSVFileProcessor:
 665:     """Processes a single CSV file and imports its records."""
 666: 
 667:     def __init__(self, file_path: Path, preview_config_loader: PreviewFieldConfigLoader,
 668:                  hash_resolver: HashResolver, db_manager: 'DatabaseConnectionManager',
 669:                  batch_size: int = 100) -> None:
 670:         self.file_path = file_path
 671:         self.preview_config_loader = preview_config_loader
 672:         self.hash_resolver = hash_resolver
 673:         self.db_manager = db_manager
 674:         self.batch_size = batch_size
 675: 
 676:     def process(self) -> ImportResult:
 677:         """Process the CSV file and import records."""
 678:         result = ImportResult(filename=self.file_path.name, object_type='Unknown')
 679: 
 680:         try:
 681:             with open(self.file_path, 'r', encoding='utf-8-sig') as f:
 682:                 reader = csv.reader(f)
 683:                 headers = next(reader, None)
 684: 
 685:                 if not headers:
 686:                     result.error_message = "CSV file has no headers"
 687:                     return result
 688: 
 689:                 object_config = detect_object_type_from_headers(headers)
 690:                 if not object_config:
 691:                     result.error_message = f"Could not detect object type from headers: {headers[:5]}"
 692:                     return result
 693: 
 694:                 result.object_type = object_config.abbreviation
 695:                 logger.info(f"Detected object type: {object_config.abbreviation} ({object_config.full_name})")
 696: 
 697:                 preview_config = self.preview_config_loader.load_config(object_config.abbreviation)
 698:                 row_parser = CSVRowParser(headers, preview_config, self.hash_resolver)
 699:                 inserter = BatchInserter(self.db_manager, self.batch_size)
 700: 
 701:                 for row_num, row in enumerate(reader, start=2):
 702:                     if not row or not row[0].strip():
 703:                         continue
 704: 
 705:                     result.total_rows += 1
 706:                     try:
 707:                         parsed = row_parser.parse_row(row)
 708:                         if parsed:
 709:                             parsed['source_filename'] = self.file_path.name
 710:                             inserter.add(parsed)
 711:                     except Exception as e:
 712:                         logger.warning(f"Row {row_num}: Parse error - {e}")
 713:                         result.error_count += 1
 714: 
 715:                 inserter.flush()
 716:                 stats = inserter.get_stats()
 717:                 result.imported_count = stats['inserted']
 718:                 result.duplicate_count = stats['duplicates']
 719:                 result.success = result.error_count == 0
 720: 
 721:                 logger.info(
 722:                     f"Import complete for {self.file_path.name}: "
 723:                     f"{result.imported_count} imported, "
 724:                     f"{result.duplicate_count} duplicates, "
 725:                     f"{result.error_count} errors"
 726:                 )
 727: 
 728:         except Exception as e:
 729:             result.error_message = str(e)
 730:             logger.error(f"Failed to process {self.file_path.name}: {e}")
 731: 
 732:         return result
 733: 
 734:     def process_update(self) -> UpdateResult:
 735:         """Process the CSV file in update mode - fill missing fields only."""
 736:         result = UpdateResult(filename=self.file_path.name, object_type='Unknown')
 737: 
 738:         try:
 739:             with open(self.file_path, 'r', encoding='utf-8-sig') as f:
 740:                 reader = csv.reader(f)
 741:                 headers = next(reader, None)
 742: 
 743:                 if not headers:
 744:                     result.error_message = "CSV file has no headers"
 745:                     return result
 746: 
 747:                 object_config = detect_object_type_from_headers(headers)
 748:                 if not object_config:
 749:                     result.error_message = f"Could not detect object type from headers: {headers[:5]}"
 750:                     return result
 751: 
 752:                 result.object_type = object_config.abbreviation
