 374: class BatchInserter:
 375:     """
 376:     Handles batch insertion of records into the database.
 377: 
 378:     Uses INSERT ... ON CONFLICT DO NOTHING to efficiently skip duplicates.
 379:     """
 380: 
 381:     def __init__(self, db_manager: 'DatabaseConnectionManager', batch_size: int = 100) -> None:
 382:         self.db_manager = db_manager
 383:         self.batch_size = batch_size
 384:         self._pending: List[Dict[str, Any]] = []
 385:         self._inserted_count: int = 0
 386:         self._duplicate_count: int = 0
 387: 
 388:     def add(self, record: Dict[str, Any]) -> None:
 389:         """Add a record to the batch"""
 390:         if record and record.get('tip'):
 391:             self._pending.append(record)
 392: 
 393:         if len(self._pending) >= self.batch_size:
 394:             self.flush()
 395: 
 396:     def flush(self) -> Tuple[int, int]:
 397:         """Flush pending records to database."""
 398:         if not self._pending:
 399:             return (0, 0)
 400: 
 401:         batch_inserted = 0
 402:         batch_duplicates = 0
 403: 
 404:         tips = [r['tip'] for r in self._pending]
 405:         existing_tips = self._get_existing_tips(tips)
 406: 
 407:         new_records = [r for r in self._pending if r['tip'] not in existing_tips]
 408:         batch_duplicates = len(self._pending) - len(new_records)
 409: 
 410:         if new_records:
 411:             batch_inserted = self._insert_batch(new_records)
 412: 
 413:         self._inserted_count += batch_inserted
 414:         self._duplicate_count += batch_duplicates
 415:         self._pending.clear()
 416: 
 417:         return (batch_inserted, batch_duplicates)
 418: 
 419:     def _get_existing_tips(self, tips: List[str]) -> set:
 420:         """Check which TIPs already exist in database"""
 421:         if not tips:
 422:             return set()
 423: 
 424:         placeholders = ','.join(['%s'] * len(tips))
 425:         query = f"SELECT tip FROM noggin_data WHERE tip IN ({placeholders})"
 426: 
 427:         try:
 428:             rows = self.db_manager.execute_query_dict(query, tuple(tips))
 429:             return {row['tip'] for row in rows}
 430:         except Exception as e:
 431:             logger.error(f"Error checking existing TIPs: {e}")
 432:             return set()
 433: 
 434:     def _insert_batch(self, records: List[Dict[str, Any]]) -> int:
 435:         """Insert a batch of records"""
 436:         if not records:
 437:             return 0
 438: 
 439:         current_time = datetime.now()
 440: 
 441:         columns = [
 442:             'tip',
 443:             'object_type',
 444:             'processing_status',
 445:             'csv_imported_at',
 446:             'created_at',
 447:             'updated_at',
 448:             'source_filename',
 449:             'expected_inspection_id',
 450:             'expected_inspection_date'
 451:         ]
 452: 
 453:         placeholders = ', '.join(['%s'] * len(columns))
 454: 
 455:         insert_sql = f"""
 456:             INSERT INTO noggin_data ({', '.join(columns)})
 457:             VALUES ({placeholders})
 458:             ON CONFLICT (tip) DO NOTHING
 459:         """
 460: 
 461:         inserted = 0
 462:         for record in records:
 463:             values = (
 464:                 record.get('tip'),
 465:                 record.get('object_type'),
 466:                 'csv_imported',
 467:                 current_time,
 468:                 current_time,
 469:                 current_time,
 470:                 record.get('source_filename'),
 471:                 record.get('expected_inspection_id'),
 472:                 record.get('expected_inspection_date')
 473:             )
 474: 
 475:             try:
 476:                 result = self.db_manager.execute_update(insert_sql, values)
 477:                 if result > 0:
 478:                     inserted += 1
 479:             except Exception as e:
 480:                 logger.error(f"Insert failed for TIP {record.get('tip', 'unknown')[:16]}...: {e}")
 481: 
 482:         return inserted
 483: 
 484:     def get_stats(self) -> Dict[str, int]:
 485:         """Get insertion statistics"""
 486:         return {
 487:             'inserted': self._inserted_count,
 488:             'duplicates': self._duplicate_count,
 489:             'pending': len(self._pending)
 490:         }
 491: 
