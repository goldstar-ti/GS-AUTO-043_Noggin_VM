276:     def _process_single_tip(self, tip_value: str) -> bool:
277:         """
278:         Process a single TIP
279:         
280:         Returns:
281:             True if processed successfully (or non-retriable error)
282:         """
283:         logger.info(f"Processing TIP: {tip_value}")
284:         
285:         # Build API URL
286:         request_url = self.base_url + self.endpoint_template.replace('$tip', tip_value)
287:         
288:         try:
289:             # Circuit breaker check
290:             from common import CircuitBreakerError
291:             
292:             try:
293:                 self.circuit_breaker.before_request()
294:             except CircuitBreakerError as e:
295:                 logger.warning(f"Circuit breaker open: {e}")
296:                 return False
297:             
298:             # Make API request
299:             response = self.api_client.make_request(request_url, tip_value)
300:             
301:             if response.status_code == 200:
302:                 self.circuit_breaker.record_success()
303:                 return self._handle_successful_response(response, tip_value)
304:             
305:             elif response.status_code == 429:
306:                 # Rate limited
307:                 self.circuit_breaker.record_failure()
308:                 sleep_time = self.api_client.too_many_requests_sleep
309:                 logger.warning(f"Rate limited, sleeping {sleep_time}s")
310:                 time.sleep(sleep_time)
311:                 return False
312:             
313:             elif response.status_code == 404:
314:                 # Not found - mark as permanent failure
315:                 error_msg = self.api_client.handle_error(response, tip_value, request_url)
316:                 logger.warning(error_msg)
317:                 self.record_manager.update_processing_status(tip_value, 'not_found', error_msg)
318:                 return True  # Don't retry 404s
319:             
320:             else:
321:                 # Other error
322:                 self.circuit_breaker.record_failure()
323:                 error_msg = self.api_client.handle_error(response, tip_value, request_url)
324:                 logger.error(error_msg)
325:                 self._handle_api_error(tip_value, error_msg)
326:                 return False
327:         
328:         except Exception as e:
329:             error_msg = f"Exception processing TIP {tip_value}: {e}"
330:             logger.error(error_msg, exc_info=True)
331:             self._handle_api_error(tip_value, error_msg)
332:             return False
333:     
334:     def _handle_successful_response(self, response, tip_value: str) -> bool:
335:         """Handle successful API response"""
336:         try:
337:             response_data = response.json()
338:         except json.JSONDecodeError as e:
339:             logger.error(f"Invalid JSON response for TIP {tip_value}: {e}")
340:             self.record_manager.update_processing_status(tip_value, 'api_error', f"Invalid JSON: {e}")
341:             return False
342:         
343:         # Extract inspection ID
344:         inspection_id = self.field_processor.extract_inspection_id(response_data) or tip_value[:16]
345:         
346:         # Insert/update database record
347:         try:
348:             self.record_manager.insert_or_update_record(response_data, tip_value)
349:         except Exception as e:
350:             logger.error(f"Failed to insert record for TIP {tip_value}: {e}")
351:             return False
352:         
353:         # Process attachments and generate report
354:         self._process_attachments_and_report(response_data, inspection_id, tip_value)
355:         
356:         return True
357:     
358:     def _process_attachments_and_report(self, response_data: Dict[str, Any],
359:                                         inspection_id: str, tip_value: str) -> None:
360:         """Process attachments and generate report"""
361:         # Get date for folder creation
362:         date_str = response_data.get('date', '')
363:         
364:         # Create inspection folder
365:         inspection_folder = self.folder_manager.create_inspection_folder(date_str, inspection_id)
366:         
367:         # Generate and save report
368:         # PASSED date_str to save_report for filename generation
369:         report = self.report_generator.generate_report(response_data, inspection_id)
370:         self.report_generator.save_report(report, inspection_folder, inspection_id, date_str)
371:         
372:         # Process attachments
373:         attachments = response_data.get('attachments', [])
374:         
375:         if not attachments:
376:             logger.info(f"No attachments for {inspection_id}")
377:             self._log_session_record(tip_value, inspection_id, 0, [])
378:             self.record_manager.update_attachment_counts(tip_value, 0, 0, True)
379:             self.record_manager.update_processing_status(tip_value, 'complete')
380:             return
381:         
382:         logger.info(f"Processing {len(attachments)} attachments for {inspection_id}")
383:         
384:         # Download attachments
385:         successful_downloads = 0
386:         attachment_filenames: List[str] = []
387:         
388:         for i, attachment_url in enumerate(attachments, 1):
389:             if not self.shutdown_handler.should_continue():
390:                 logger.warning(f"Shutdown during attachment {i}/{len(attachments)}")
391:                 break
392:             
393:             # Extract attachment TIP
394:             attachment_tip = attachment_url.split('tip=')[-1] if 'tip=' in attachment_url else f'unknown_{i}'
395:             
396:             # Construct filename
397:             filename = self.folder_manager.construct_attachment_filename(
398:                 inspection_id, date_str, i
399:             )
400:             
401:             # Download
402:             success, retry_count, file_size_mb, error_msg = self.attachment_downloader.download(
403:                 attachment_url, filename, inspection_id, attachment_tip,
404:                 inspection_folder, tip_value, i
405:             )
406:             
407:             if success:
408:                 successful_downloads += 1
409:                 attachment_filenames.append(filename)
410:             
411:             # Pause between attachments
412:             if self.attachment_pause > 0 and i < len(attachments):
413:                 time.sleep(self.attachment_pause)
414:         
415:         # Determine final status
416:         if not self.shutdown_handler.should_continue():
417:             final_status = 'interrupted'
418:         elif successful_downloads == len(attachments):
419:             final_status = 'complete'
420:         elif successful_downloads > 0:
421:             final_status = 'partial'
422:         else:
423:             final_status = 'failed'
424:         
425:         # Update database
426:         all_complete = successful_downloads == len(attachments)
427:         self.record_manager.update_attachment_counts(
428:             tip_value, len(attachments), successful_downloads, all_complete
429:         )
430:         self.record_manager.update_processing_status(tip_value, final_status)
431:         
432:         # Log to session
433:         self._log_session_record(tip_value, inspection_id, successful_downloads, attachment_filenames)
434:         
435:         logger.info(f"Completed {inspection_id}: {successful_downloads}/{len(attachments)} attachments")
436:     
437:     def _handle_api_error(self, tip_value: str, error_msg: str) -> None:
438:         """Handle API error with retry logic"""
439:         # Get current retry count
440:         result = self.db_manager.execute_query_dict(
441:             "SELECT retry_count FROM noggin_data WHERE tip = %s",
442:             (tip_value,)
443:         )
444:         
445:         current_retry = result[0]['retry_count'] if result else 0
446:         new_retry = current_retry + 1
447:         
448:         if self.retry_manager.should_retry(new_retry):
449:             next_retry = self.retry_manager.calculate_next_retry_time(new_retry)
450:             self.record_manager.update_retry_info(tip_value, new_retry, next_retry)
451:             self.record_manager.update_processing_status(tip_value, 'api_error', error_msg)
452:             logger.info(f"Scheduled retry {new_retry} for TIP {tip_value} at {next_retry}")
453:         else:
454:             self.record_manager.mark_permanently_failed(tip_value, f"Max retries exceeded: {error_msg}")
455:             logger.warning(f"TIP {tip_value} permanently failed after {new_retry} retries")
456:         
457:         # Record error
458:         self.record_manager.record_processing_error(
459:             tip_value, 'api_error', error_msg,
460:             {'retry_count': new_retry}
461:         )
462:     
463:     def _log_session_record(self, tip_value: str, inspection_id: str,
464:                            attachment_count: int, filenames: List[str]) -> None:
465:         """Log to session log file"""
466:         timestamp = datetime.now().strftime('%Y-%m-%d %H:%M:%S')
467:         filenames_str = ";".join(filenames) if filenames else "NONE"
468:         
469:         self.session_logger.info(
470:             f"{timestamp}\t{tip_value}\t{inspection_id}\t{attachment_count}\t{filenames_str}"
471:         )
472:     
473:     def _count_tips_in_csv(self, csv_path: Path) -> int:
474:         """Count valid TIPs in CSV file"""
475:         count = 0
476:         
477:         with open(csv_path, 'r', encoding='utf-8') as f:
478:             reader = csv.DictReader(f)
479:             
480:             tip_column = None
481:             for col in reader.fieldnames or []:
482:                 if col.lower() == 'tip':
483:                     tip_column = col
484:                     break
485:             
486:             if not tip_column:
487:                 return 0
488:             
489:             for row in reader:
490:                 if row.get(tip_column, '').strip():
491:                     count += 1
492:         
493:         return count
