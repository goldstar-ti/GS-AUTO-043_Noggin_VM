 48:         # Output settings
 49:         self.show_json: bool = config.getboolean('output', 'show_json_payload_in_text_file', 
 50:                                                   fallback=True, from_specific=True)
 51:         self.show_all_fields: bool = config.getboolean('output', 'show_all_fields',
 52:                                                         fallback=False, from_specific=True)
 53:         self.unknown_text: str = config.get('output', 'unknown_response_output_text',
 54:                                             fallback='Unknown', from_specific=True)
 55:         self.filename_pattern: str = config.get('output', 'textfile_pattern', 
 56:                                               fallback='{inspection_id}_inspection_data.txt', 
 57:                                               from_specific=True)
 58:         
 59:         # Date format for reports
 60:         self.date_format: str = config.get('report', 'date_format', fallback='%Y-%m-%d')
 61:         
 62:         # Parse field mappings for hash resolution
 63:         self.field_mappings = config.get_field_mappings()
 64:         
 65:         # Build set of date field names for quick lookup
 66:         self.date_fields: set = {'date'}
 67:         for api_field, (db_column, field_type, hash_type) in self.field_mappings.items():
 68:             if field_type == 'datetime':
 69:                 self.date_fields.add(api_field)
