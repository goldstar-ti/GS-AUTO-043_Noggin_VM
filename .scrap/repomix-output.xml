<file path="common/config.py">
  1: import configparser
  2: from pathlib import Path
  3: from typing import Any, Optional, Tuple
  4: import logging
  5: 
  6: logger: logging.Logger = logging.getLogger(__name__)
  7: 
  8: class ConfigurationError(Exception):
  9:     pass
 10: 
 11: class ConfigLoader:
 12:     def __init__(self, base_config_path: str, specific_config_path: Optional[str] = None) -> None:
 13:         self.base_config: configparser.ConfigParser = configparser.ConfigParser()
 14:         self.specific_config: configparser.ConfigParser = configparser.ConfigParser()
 15:         
 16:         if not Path(base_config_path).exists():
 17:             raise ConfigurationError(f"Base config not found: {base_config_path}")
 18:         
 19:         self.base_config.read(base_config_path)
 20:         logger.info(f"Loaded base configuration from {base_config_path}")
 21:         
 22:         if specific_config_path:
 23:             if not Path(specific_config_path).exists():
 24:                 raise ConfigurationError(f"Specific config not found: {specific_config_path}")
 25:             
 26:             self.specific_config.read(specific_config_path)
 27:             logger.info(f"Loaded specific configuration from {specific_config_path}")
 28:         
 29:         self._validate_configuration()
 30:     
 31:     def _validate_configuration(self) -> None:
 32:         required_base_sections: dict[str, list[str]] = {
 33:             'postgresql': ['host', 'port', 'database', 'user', 'password'],
 34:             'paths': ['base_output_path', 'base_log_path', 'input_folder_path'],
 35:             'api': ['base_url', 'media_service_url', 'namespace', 'bearer_token'],
 36:             'processing': ['max_api_retries', 'api_timeout'],
 37:             'retry': ['max_retry_attempts', 'retry_backoff_multiplier']
 38:         }
 39:         
 40:         for section, keys in required_base_sections.items():
 41:             if not self.base_config.has_section(section):
 42:                 raise ConfigurationError(f"Missing required section: [{section}]")
 43:             
 44:             for key in keys:
 45:                 if not self.base_config.has_option(section, key):
 46:                     raise ConfigurationError(f"Missing required key: [{section}] {key}")
 47:         
 48:         logger.info("Configuration validation passed")
 49:     
 50:     def get(self, section: str, key: str, fallback: Optional[str] = None, from_specific: bool = False) -> str:
 51:             config: configparser.ConfigParser = self.specific_config if from_specific else self.base_config
 52:             
 53:             if from_specific and not config.has_option(section, key):
 54:                 config = self.base_config
 55:             
 56:             try:
 57:                 value: str = config.get(section, key, fallback=fallback)
 58:             except configparser.InterpolationSyntaxError:
 59:                 # If standard parsing fails (e.g. date format like "%d %b %Y"), 
 60:                 # retry getting the raw value without interpolation
 61:                 logger.debug(f"Interpolation failed for [{section}] {key}, retrieving raw value.")
 62:                 value: str = config.get(section, key, fallback=fallback, raw=True)
 63:                 
 64:             return value
 65: 
 66:     
 67:     def getint(self, section: str, key: str, fallback: Optional[int] = None, from_specific: bool = False) -> int:
 68:         config: configparser.ConfigParser = self.specific_config if from_specific else self.base_config
 69:         
 70:         if from_specific and not config.has_option(section, key):
 71:             config = self.base_config
 72:         
 73:         value: int = config.getint(section, key, fallback=fallback)
 74:         return value
 75:     
 76:     def getfloat(self, section: str, key: str, fallback: Optional[float] = None, from_specific: bool = False) -> float:
 77:         config: configparser.ConfigParser = self.specific_config if from_specific else self.base_config
 78:         
 79:         if from_specific and not config.has_option(section, key):
 80:             config = self.base_config
 81:         
 82:         value: float = config.getfloat(section, key, fallback=fallback)
 83:         return value
 84:     
 85:     def getboolean(self, section: str, key: str, fallback: Optional[bool] = None, from_specific: bool = False) -> bool:
 86:         config: configparser.ConfigParser = self.specific_config if from_specific else self.base_config
 87:         
 88:         if from_specific and not config.has_option(section, key):
 89:             config = self.base_config
 90:         
 91:         value: bool = config.getboolean(section, key, fallback=fallback)
 92:         return value
 93:     
 94:     def get_section(self, section: str, from_specific: bool = False) -> dict[str, str]:
 95:         config: configparser.ConfigParser = self.specific_config if from_specific else self.base_config
 96:         
 97:         if not config.has_section(section):
 98:             return {}
 99:         
100:         return dict(config.items(section))
101:     
102:     def get_postgresql_config(self) -> dict[str, Any]:
103:         return {
104:             'host': self.get('postgresql', 'host'),
105:             'port': self.getint('postgresql', 'port'),
106:             'database': self.get('postgresql', 'database'),
107:             'user': self.get('postgresql', 'user'),
108:             'password': self.get('postgresql', 'password'),
109:             'schema': self.get('postgresql', 'schema', fallback='noggin_schema'),
110:             'minconn': self.getint('postgresql', 'pool_min_connections', fallback=2),
111:             'maxconn': self.getint('postgresql', 'pool_max_connections', fallback=10)
112:         }
113:     
114:     def get_api_headers(self) -> dict[str, str]:
115:         return {
116:             'en-namespace': self.get('api', 'namespace'),
117:             'authorization': f"Bearer {self.get('api', 'bearer_token')}"
118:         }
119:     
120:     def get_object_type_config(self) -> dict[str, str]:
121:         # id_field format: "apiFieldName:dbColumnName:fieldType"
122:         # e.g., "lcdInspectionId:noggin_reference:string"
123:         id_field_raw = self.get('fields', 'id_field', from_specific=True)
124:         
125:         # Parse the id_field to extract components
126:         id_parts = id_field_raw.split(':') if id_field_raw else ['', '', '']
127:         api_id_field = id_parts[0] if len(id_parts) > 0 else ''
128:         db_id_column = id_parts[1] if len(id_parts) > 1 else ''
129:         
130:         return {
131:             'endpoint': self.get('api', 'endpoint', from_specific=True),
132:             'object_type': self.get('api', 'object_type', from_specific=True),
133:             'abbreviation': self.get('object_type', 'abbreviation', from_specific=True),
134:             'full_name': self.get('object_type', 'full_name', from_specific=True),
135:             # Added inspection_type here
136:             'inspection_type': self.get('object_type', 'inspection_type', from_specific=True, fallback='Inspection'),
137:             'id_field': id_field_raw,
138:             'api_id_field': api_id_field,
139:             'db_id_column': db_id_column,
140:         }
141:     
142:     def get_field_mappings(self) -> dict[str, tuple[str, str, Optional[str]]]:
143:         """
144:         Parse [fields] section and return field mappings.
145:         
146:         Config format: api_field = db_column:field_type[:hash_type]
147:         Returns: {api_field: (db_column, field_type, hash_type or None)}
148:         """
149:         fields_section = self.get_section('fields', from_specific=True)
150:         mappings: dict[str, tuple[str, str, Optional[str]]] = {}
151:         
152:         # Skip meta fields (id_field, date_field)
153:         skip_keys = {'id_field', 'date_field'}
154:         
155:         for api_field, value in fields_section.items():
156:             if api_field in skip_keys:
157:                 continue
158:             
159:             parts = value.split(':')
160:             if len(parts) < 2:
161:                 logger.warning(f"Invalid field mapping for {api_field}: {value}")
162:                 continue
163:             
164:             db_column = parts[0]
165:             field_type = parts[1]
166:             hash_type = parts[2] if len(parts) > 2 else None
167:             
168:             mappings[api_field] = (db_column, field_type, hash_type)
169:         
170:         return mappings
171:     
172:     def get_output_patterns(self) -> dict[str, str]:
173:         """Get output patterns from [output] section"""
174:         return self.get_section('output', from_specific=True)
175:     
176:     def get_template_content(self) -> Optional[str]:
177:         """Get report template content from [template] section"""
178:         if self.specific_config.has_option('template', 'content'):
179:             return self.specific_config.get('template', 'content')
180:         return None
181: 
