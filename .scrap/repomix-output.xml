 108: class HashResolver:
 109:     """
 110:     Resolves hash values to human-readable text using the hash_lookup table.
 111: 
 112:     Caches lookups to minimise database queries during batch imports.
 113:     """
 114: 
 115:     HASH_PATTERN = re.compile(r'^[a-fA-F0-9]{64}$')
 116: 
 117:     def __init__(self, db_manager: 'DatabaseConnectionManager') -> None:
 118:         self.db_manager = db_manager
 119:         self._cache: Dict[str, Optional[str]] = {}
 120:         self._cache_hits: int = 0
 121:         self._cache_misses: int = 0
 122: 
 123:     def is_hash(self, value: str) -> bool:
 124:         """Check if a value appears to be a 64-character hash"""
 125:         if not value or not isinstance(value, str):
 126:             return False
 127:         return bool(self.HASH_PATTERN.match(value.strip()))
 128: 
 129:     def resolve(self, hash_value: str) -> Optional[str]:
 130:         """Resolve a hash to its human-readable value."""
 131:         if not hash_value:
 132:             return None
 133: 
 134:         hash_value = hash_value.strip()
 135: 
 136:         if hash_value in self._cache:
 137:             self._cache_hits += 1
 138:             return self._cache[hash_value]
 139: 
 140:         self._cache_misses += 1
 141: 
 142:         try:
 143:             rows = self.db_manager.execute_query_dict(
 144:                 "SELECT resolved_value FROM hash_lookup WHERE tip_hash = %s",
 145:                 (hash_value,)
 146:             )
 147: 
 148:             if rows:
 149:                 resolved = rows[0]['resolved_value']
 150:                 self._cache[hash_value] = resolved
 151:                 return resolved
 152:             else:
 153:                 self._cache[hash_value] = None
 154:                 return None
 155: 
 156:         except Exception as e:
 157:             logger.warning(f"Hash lookup failed for {hash_value[:16]}...: {e}")
 158:             return None
 159: 
 160:     def resolve_or_passthrough(self, value: str) -> Tuple[Optional[str], Optional[str]]:
 161:         """
 162:         Resolve a value if it's a hash, otherwise pass through.
 163: 
 164:         Returns:
 165:             Tuple of (resolved_value, hash_value)
 166:             - If input is a hash: (resolved_text or None, original_hash)
 167:             - If input is text: (original_text, None)
 168:         """
 169:         if not value:
 170:             return (None, None)
 171: 
 172:         value = str(value).strip()
 173: 
 174:         if self.is_hash(value):
 175:             resolved = self.resolve(value)
 176:             return (resolved, value)
 177:         else:
 178:             return (value, None)
 179: 
 180:     def get_cache_stats(self) -> Dict[str, int]:
 181:         """Return cache statistics"""
 182:         return {
 183:             'cache_size': len(self._cache),
 184:             'cache_hits': self._cache_hits,
 185:             'cache_misses': self._cache_misses
 186:         }
 187: 
 188:     def clear_cache(self) -> None:
 189:         """Clear the resolution cache"""
 190:         self._cache.clear()
 191:         self._cache_hits = 0
 192:         self._cache_misses = 0
 193: 
