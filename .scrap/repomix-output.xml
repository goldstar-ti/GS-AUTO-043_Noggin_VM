140:     def _format_date(self, date_value: str) -> str:
141:         """
142:         Format ISO date string to configured format.
143:         
144:         Args:
145:             date_value: ISO format date string (e.g., '2025-11-28T00:00:00.000+08:00')
146:             
147:         Returns:
148:             Formatted date string or original if parsing fails
149:         """
150:         if not date_value or not isinstance(date_value, str):
151:             return str(date_value) if date_value else self.unknown_text
152:         
153:         try:
154:             # Handle various ISO formats
155:             clean_date = date_value.replace('Z', '+00:00')
156:             
157:             # Try parsing with timezone
158:             if '+' in clean_date or '-' in clean_date[10:]:
159:                 # Has timezone info
160:                 parsed = datetime.fromisoformat(clean_date)
161:             else:
162:                 parsed = datetime.fromisoformat(clean_date)
163:             
164:             return parsed.strftime(self.date_format)
165:         except (ValueError, AttributeError) as e:
166:             logger.debug(f"Could not parse date '{date_value}': {e}")
167:             return date_value
168:     
169:     def _process_template(self, template: str, context: Dict[str, Any]) -> str:
170:         """Process template with context substitution"""
171:         result = template
172:         
173:         # Process conditional blocks first (nested supported)
174:         result = self._process_conditionals(result, context)
175:         
176:         # Replace placeholders
177:         result = self._replace_placeholders(result, context)
178:         
179:         return result
180:     
181:     def _process_conditionals(self, template: str, context: Dict[str, Any]) -> str:
182:         """
183:         Process conditional blocks <if:field>...</if:field>
184:         
185:         Supports nested conditionals.
186:         """
187:         # Regex for conditional blocks (non-greedy, handles nesting by processing innermost first)
188:         pattern = r'<if:(\w+)>(.*?)</if:\1>'
189:         
190:         max_iterations = 10  # Prevent infinite loops
191:         iteration = 0
192:         
193:         while iteration < max_iterations:
194:             matches = list(re.finditer(pattern, template, re.DOTALL))
195:             if not matches:
196:                 break
197:             
198:             # Process from end to start to preserve positions
199:             for match in reversed(matches):
200:                 field_name = match.group(1)
201:                 content = match.group(2)
202:                 
203:                 # Check if condition is met
204:                 should_include = self._evaluate_condition(field_name, context)
205:                 
206:                 if should_include:
207:                     # Include content (will be processed for nested conditionals in next iteration)
208:                     replacement = content
209:                 else:
210:                     # Remove entire block
211:                     replacement = ''
212:                 
213:                 template = template[:match.start()] + replacement + template[match.end():]
214:             
215:             iteration += 1
216:         
217:         return template
218:     
219:     def _evaluate_condition(self, field_name: str, context: Dict[str, Any]) -> bool:
220:         """Evaluate if a conditional field should be included"""
221:         value = context.get(field_name)
222:         
223:         if value is None:
224:             return False
225:         
226:         if isinstance(value, bool):
227:             return value
228:         
229:         if isinstance(value, str):
230:             # Empty string or "Unknown" = false
231:             return bool(value) and value != self.unknown_text
232:         
233:         if isinstance(value, (list, dict)):
234:             return len(value) > 0
235:         
236:         # Numbers: 0 = false, anything else = true
237:         if isinstance(value, (int, float)):
238:             return value != 0
239:         
240:         return bool(value)
241:     
242:     def _replace_placeholders(self, template: str, context: Dict[str, Any]) -> str:
243:         """Replace <placeholder> with values from context"""
244:         
245:         def replacer(match):
246:             field_name = match.group(1)
247:             value = context.get(field_name)
248:             
249:             if value is None:
250:                 return self.unknown_text
251:             
252:             if isinstance(value, bool):
253:                 return 'Yes' if value else 'No'
254:             
255:             if isinstance(value, datetime):
256:                 return value.strftime('%Y-%m-%d %H:%M:%S')
257:             
258:             if isinstance(value, (dict, list)):
259:                 return json.dumps(value, indent=2, ensure_ascii=False)
260:             
261:             return str(value)
262:         
263:         # Match <field_name> but not <if: or </if:
264:         pattern = r'<(?!if:|/if:)(\w+)>'
265:         
266:         return re.sub(pattern, replacer, template)
267:     
268:     def save_report(self, report: str, inspection_folder: Path, 
269:                    inspection_id: str, date_str: Optional[str] = None) -> Optional[Path]:
270:         """
271:         Save report to file using configured filename pattern
272:         
273:         Args:
274:             report: Report content
275:             inspection_folder: Folder to save in
276:             inspection_id: Used for filename
277:             date_str: ISO date string for filename substitution
278:             
279:         Returns:
280:             Path to saved file or None on error
281:         """
282:         # Parse date for filename components
283:         try:
284:             if date_str:
285:                 date_obj = datetime.fromisoformat(date_str.replace('Z', '+00:00'))
286:                 date_formatted = date_obj.strftime('%Y-%m-%d')
287:                 year = date_obj.strftime('%Y')
288:                 month = date_obj.strftime('%m')
289:             else:
290:                 raise ValueError("Empty date")
291:         except (ValueError, AttributeError):
292:             date_formatted = 'unknown_date'
293:             year = 'unknown'
294:             month = 'unknown'
295:             
296:         # Handle user-requested spaces in keys (e.g. {inspection type}) by normalizing pattern
297:         # This converts "{inspection id}" to "{inspection_id}" before python formatting
298:         pattern = self.filename_pattern
299:         pattern = pattern.replace('{inspection id}', '{inspection_id}')
300:         pattern = pattern.replace('{inspection type}', '{inspection_type}')
301:         pattern = pattern.replace('{full name}', '{full_name}')
302:         
303:         try:
304:             filename = pattern.format(
305:                 full_name=self.full_name,
306:                 abbreviation=self.abbreviation,
307:                 inspection_id=inspection_id,
308:                 inspection_type=self.inspection_type,
309:                 date=date_formatted,
310:                 year=year,
311:                 month=month
312:             )
313:         except KeyError as e:
314:             logger.warning(f"Invalid key in filename pattern '{self.filename_pattern}': {e}. Using default.")
315:             filename = f"{inspection_id}_inspection_data.txt"
316:         
317:         # Sanitise the resulting filename
318:         # This uses the version from base_processor which allows spaces
319:         filename = sanitise_filename(filename)
320:         
321:         # Ensure extension
322:         if not filename.lower().endswith('.txt'):
323:             filename += ".txt"
324:         
325:         file_path = inspection_folder / filename
326:         
327:         try:
328:             with open(file_path, 'w', encoding='utf-8') as f:
329:                 f.write(report)
330:             
331:             logger.info(f"Saved report to: {file_path}")
332:             return file_path
333:             
334:         except IOError as e:
335:             logger.error(f"Failed to save report to {file_path}: {e}")
336:             return None
337: 
338: 
339: class DefaultReportGenerator:
340:     """
341:     Fallback report generator when no template is configured
342:     
343:     Generates a simple key-value report from the API response.
344:     """
345:     
346:     def __init__(self, config: 'ConfigLoader', hash_manager: 'HashManager') -> None:
347:         self.config = config
348:         self.hash_manager = hash_manager
349:         
350:         obj_config = config.get_object_type_config()
351:         self.full_name: str = obj_config['full_name']
352:         self.abbreviation: str = obj_config['abbreviation']
353:         self.inspection_type: str = obj_config.get('inspection_type', 'Inspection')
354:         
355:         self.show_json: bool = config.getboolean('output', 'show_json_payload_in_text_file',
356:                                                   fallback=True, from_specific=True)
357:         self.unknown_text: str = config.get('output', 'unknown_response_output_text',
358:                                             fallback='Unknown', from_specific=True)
359:         self.filename_pattern: str = config.get('output', 'textfile_pattern', 
360:                                               fallback='{inspection_id}_inspection_data.txt', 
361:                                               from_specific=True)
362:         
363:         # Date format for reports
364:         self.date_format: str = config.get('report', 'date_format', fallback='%Y-%m-%d')
365:         
366:         self.field_mappings = config.get_field_mappings()
367:         
368:         # Build set of date field names
369:         self.date_fields: set = {'date'}
370:         for api_field, (db_column, field_type, hash_type) in self.field_mappings.items():
371:             if field_type == 'datetime':
372:                 self.date_fields.add(api_field)
373:     
374:     def generate_report(self, response_data: Dict[str, Any],
375:                        inspection_id: str) -> str:
376:         """Generate a default report without template"""
377:         lines: List[str] = []
378:         
379:         lines.append("=" * 60)
380:         lines.append(self.full_name.upper())
381:         lines.append(f"RECORD GENERATED: {datetime.now().strftime('%d-%m-%Y')}")
382:         lines.append("=" * 60)
383:         lines.append("")
384:         
385:         tip_value = response_data.get('$meta', {}).get('tip', '')
386:         
387:         # Output all mapped fields
388:         for api_field, (db_column, field_type, hash_type) in self.field_mappings.items():
389:             value = response_data.get(api_field)
390:             
391:             if value is None:
392:                 continue
393:             
394:             # Format field name nicely
395:             display_name = self._format_field_name(api_field)
396:             
397:             if field_type == 'hash' and hash_type:
398:                 resolved = self.hash_manager.lookup_hash(
399:                     hash_type, str(value), tip_value, inspection_id
400:                 )
401:                 lines.append(f"{display_name}: {resolved}")
402:             elif field_type == 'bool':
403:                 lines.append(f"{display_name}: {'Yes' if value else 'No'}")
404:             elif field_type == 'datetime' or api_field in self.date_fields:
405:                 lines.append(f"{display_name}: {self._format_date(value)}")
406:             else:
407:                 lines.append(f"{display_name}: {value}")
408:         
409:         lines.append("")
410:         
411:         # Attachments
412:         attachment_count = len(response_data.get('attachments', []))
413:         lines.append(f"Attachments: {attachment_count}")
414:         lines.append("")
415:         
416:         # Optional JSON payload
417:         if self.show_json:
418:             lines.append("-" * 60)
419:             lines.append("COMPLETE TECHNICAL DATA (JSON FORMAT)")
420:             lines.append("-" * 60)
421:             lines.append("")
422:             lines.append(json.dumps(response_data, indent=2, ensure_ascii=False))
423:         
424:         return '\n'.join(lines)
425:     
426:     def _format_field_name(self, field_name: str) -> str:
427:         """Convert camelCase to Title Case with spaces"""
428:         # Insert space before capitals
429:         spaced = re.sub(r'([A-Z])', r' \1', field_name)
430:         # Handle consecutive capitals (e.g., "ID" -> "ID" not "I D")
431:         spaced = re.sub(r'([A-Z]+)([A-Z][a-z])', r'\1 \2', spaced)
432:         # Title case and strip
433:         return spaced.strip().title()
434:     
435:     def _format_date(self, date_value: str) -> str:
436:         """Format ISO date string to configured format"""
437:         if not date_value or not isinstance(date_value, str):
438:             return str(date_value) if date_value else self.unknown_text
439:         
440:         try:
441:             clean_date = date_value.replace('Z', '+00:00')
442:             parsed = datetime.fromisoformat(clean_date)
443:             return parsed.strftime(self.date_format)
444:         except (ValueError, AttributeError):
445:             return date_value
446:     
447:     def save_report(self, report: str, inspection_folder: Path,
448:                    inspection_id: str, date_str: Optional[str] = None) -> Optional[Path]:
449:         """Save report to file"""
450:         # Logic duplicated from ReportGenerator.save_report
451:         # Ideally this would be a mixin or base class, but keeping it simple for now
452:         
453:         try:
454:             if date_str:
455:                 date_obj = datetime.fromisoformat(date_str.replace('Z', '+00:00'))
456:                 date_formatted = date_obj.strftime('%Y-%m-%d')
457:                 year = date_obj.strftime('%Y')
458:                 month = date_obj.strftime('%m')
459:             else:
460:                 raise ValueError("Empty date")
461:         except (ValueError, AttributeError):
462:             date_formatted = 'unknown_date'
463:             year = 'unknown'
464:             month = 'unknown'
465:             
466:         pattern = self.filename_pattern
467:         pattern = pattern.replace('{inspection id}', '{inspection_id}')
468:         pattern = pattern.replace('{inspection type}', '{inspection_type}')
469:         pattern = pattern.replace('{full name}', '{full_name}')
470:         
471:         try:
472:             filename = pattern.format(
473:                 full_name=self.full_name,
474:                 abbreviation=self.abbreviation,
475:                 inspection_id=inspection_id,
476:                 inspection_type=self.inspection_type,
477:                 date=date_formatted,
478:                 year=year,
479:                 month=month
480:             )
481:         except KeyError as e:
482:             logger.warning(f"Invalid key in filename pattern '{self.filename_pattern}': {e}. Using default.")
483:             filename = f"{inspection_id}_inspection_data.txt"
484: 
485:         filename = sanitise_filename(filename)
486:         
487:         if not filename.lower().endswith('.txt'):
488:             filename += ".txt"
489:         
490:         file_path = inspection_folder / filename
491:         
492:         try:
493:             with open(file_path, 'w', encoding='utf-8') as f:
494:                 f.write(report)
495:             logger.info(f"Saved report to: {file_path}")
496:             return file_path
497:         except IOError as e:
498:             logger.error(f"Failed to save report: {e}")
499:             return None
500: 
501: 
502: def create_report_generator(config: 'ConfigLoader', 
503:                            hash_manager: 'HashManager') -> ReportGenerator:
504:     """
505:     Factory function to create appropriate report generator
506:     
507:     Returns ReportGenerator if template configured, else DefaultReportGenerator.
508:     """
509:     try:
510:         template = config.get_template_content()
511:         if template and template.strip():
512:             return ReportGenerator(config, hash_manager)
513:     except Exception as e:
514:         logger.warning(f"Could not load template, using default: {e}")
515:     
516:     return DefaultReportGenerator(config, hash_manager)
