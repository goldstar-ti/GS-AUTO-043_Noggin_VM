 92:                     {% if item.retry_count and item.retry_count > 0 %}
 93:                         <span class="badge warning" style="font-size: 0.7em; padding: 2px 5px;">R:{{ item.retry_count }}</span>
 94:                     {% endif %}
 95:                     
 96:                     <br>
 97:                     <small style="color: #7f8c8d;">{{ item.object_type }}</small>
 98:                 </td>
 99:                 
100:                 <!-- Stacked User & Team Column -->
101:                 <td>
102:                     <div style="font-weight: 500;">
103:                         {% if item.derived_user %}
104:                             {{ item.derived_user }}
105:                         {% else %}
106:                             <span style="color: #ccc;">-</span>
107:                         {% endif %}
108:                     </div>
109:                     
110:                     <div style="font-size: 0.85em; color: #7f8c8d;">
111:                         {% if item.team %}
112:                             {{ item.team }}
113:                         {% elif item.department %}
114:                             {{ item.department }}
115:                         {% endif %}
116:                     </div>
117:                 </td>
118: 
119:                 <!-- Date with Year -->
