 97: def sanitise_filename(text: str) -> str:
 98:     """
 99:     Sanitise string for use in filenames.
100:     Allows spaces to preserve formatting like 'TA - 00014'.
101:     """
102:     if not text:
103:         return "unknown"
104:     
105:     # Replace illegal filename characters with underscore
106:     sanitised: str = re.sub(r'[<>:"/\\|?*]', '_', str(text))
107:     
108:     # Replace whitespace characters (tabs, newlines) with standard space
109:     sanitised = re.sub(r'[\t\r\n]+', ' ', sanitised)
110:     
111:     # Collapse multiple spaces into one
112:     sanitised = re.sub(r'\s+', ' ', sanitised)
113:     
114:     # Remove leading/trailing spaces or underscores
115:     sanitised = sanitised.strip('_ ')
116:     
117:     return sanitised[:100] if sanitised else "unknown"
