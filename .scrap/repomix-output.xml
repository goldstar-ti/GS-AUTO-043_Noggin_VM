 73:     def generate_report(self, response_data: Dict[str, Any], 
 74:                        inspection_id: str) -> str:
 75:         """
 76:         Generate report from template and API response
 77:         
 78:         Args:
 79:             response_data: API response JSON
 80:             inspection_id: Inspection ID for hash lookups
 81:             
 82:         Returns:
 83:             Formatted report string
 84:         """
 85:         # Build context with all available values
 86:         context = self._build_context(response_data, inspection_id)
 87:         
 88:         # Process template
 89:         report = self._process_template(self.template, context)
 90:         
 91:         # Clean up extra blank lines
 92:         report = re.sub(r'\n{3,}', '\n\n', report)
 93:         
 94:         return report
