499: class ProgressTracker:
500:     """Tracks processing progress and estimates"""
501:     
502:     def __init__(self, total_count: int) -> None:
503:         self.total_count: int = total_count
504:         self.processed_count: int = 0
505:         self.start_time: float = time.perf_counter()
506:         self.last_log_time: float = self.start_time
507:         self.log_interval: int = 10  # seconds
508:     
509:     def increment(self) -> None:
510:         self.processed_count += 1
511:     
512:     def should_log_progress(self) -> bool:
513:         current_time = time.perf_counter()
514:         if current_time - self.last_log_time >= self.log_interval:
515:             self.last_log_time = current_time
516:             return True
517:         return False
518:     
519:     def get_progress_stats(self) -> Dict[str, Any]:
520:         """Get current progress statistics"""
521:         elapsed: float = time.perf_counter() - self.start_time
522:         
523:         if self.processed_count > 0:
524:             rate: float = self.processed_count / elapsed
525:             remaining: int = self.total_count - self.processed_count
526:             eta_seconds: float = remaining / rate if rate > 0 else 0
527:         else:
528:             rate = 0
529:             eta_seconds = 0
530:         
531:         return {
532:             'processed': self.processed_count,
533:             'total': self.total_count,
534:             'elapsed_seconds': elapsed,
535:             'rate_per_second': rate,
536:             'eta_seconds': eta_seconds,
537:             'percent_complete': (self.processed_count / self.total_count * 100) if self.total_count > 0 else 0
538:         }
539:     
540:     def log_progress(self) -> None:
541:         """Log current progress"""
542:         stats = self.get_progress_stats()
543:         
544:         eta_minutes: float = stats['eta_seconds'] / 60
545:         elapsed_minutes: float = stats['elapsed_seconds'] / 60
546:         
547:         logger.info(
548:             f"Progress: {stats['processed']}/{stats['total']} "
549:             f"({stats['percent_complete']:.1f}%) - "
550:             f"{stats['rate_per_second']:.2f} TIPs/sec - "
551:             f"Elapsed: {elapsed_minutes:.1f}m - "
552:             f"ETA: {eta_minutes:.1f}m"
553:         )
554:     
555:     def log_shutdown_summary(self, reason: str = "complete") -> None:
556:         """Log final summary"""
557:         stats = self.get_progress_stats()
558:         elapsed_minutes: float = stats['elapsed_seconds'] / 60
559:         
560:         logger.info("=" * 60)
561:         logger.info(f"PROCESSING {reason.upper()}")
562:         logger.info(f"Processed: {stats['processed']}/{stats['total']} TIPs")
563:         logger.info(f"Duration: {elapsed_minutes:.1f} minutes")
564:         logger.info(f"Average rate: {stats['rate_per_second']:.2f} TIPs/sec")
565:         logger.info("=" * 60)
