272: class AttachmentDownloader:
273:     """Handles attachment downloading with validation and database tracking"""
274:     
275:     def __init__(self, config: 'ConfigLoader', db_manager: 'DatabaseConnectionManager',
276:                  api_client: APIClient) -> None:
277:         self.config = config
278:         self.db_manager = db_manager
279:         self.api_client = api_client
280:         
281:         self.attachment_pause: int = config.getint('processing', 'attachment_pause')
282:         self.min_file_size: int = config.getint('processing', 'min_attachment_size', fallback=1024)
283:     
284:     def download(self, attachment_url: str, filename: str, inspection_id: str,
285:                 attachment_tip: str, inspection_folder: Path,
286:                 record_tip: str, attachment_sequence: int) -> Tuple[bool, int, float, Optional[str]]:
287:         """
288:         Download and validate attachment with database tracking
289:         
290:         Returns:
291:             Tuple of (success, retry_count, file_size_mb, error_message)
292:         """
293:         if attachment_url.startswith('/media'):
294:             attachment_url = attachment_url[6:]
295: 
296:         full_url: str = self.api_client.attachment_base_url + attachment_url
297:         output_path: Path = inspection_folder / filename
298:         temp_path: Path = output_path.with_suffix('.tmp')
299: 
300:         download_start: datetime = datetime.now()
301: 
302:         # Insert initial attachment record
303:         try:
304:             self.db_manager.execute_update(
305:                 """
306:                 INSERT INTO attachments (record_tip, attachment_tip, attachment_sequence, filename, 
307:                                         file_path, attachment_status, attachment_validation_status, download_started_at)
308:                 VALUES (%s, %s, %s, %s, %s, %s, 'pending', %s)
309:                 ON CONFLICT (record_tip, attachment_tip) DO UPDATE SET
310:                     attachment_status = 'downloading',
311:                     attachment_validation_status = 'pending',
312:                     download_started_at = EXCLUDED.download_started_at,
313:                     filename = EXCLUDED.filename,
314:                     file_path = EXCLUDED.file_path
315:                 """,
316:                 (record_tip, attachment_tip, attachment_sequence, filename, 
317:                  str(output_path), 'downloading', download_start)
318:             )
319:         except Exception as e:
320:             logger.warning(f"Could not insert attachment record: {e}")
321: 
322:         try:
323:             response = self.api_client.make_request(full_url, attachment_tip)
324:             retry_count: int = getattr(response, '_retry_count', 0)
325: 
326:             if response.status_code != 200:
327:                 error_msg = self.api_client.handle_error(response, attachment_tip, full_url)
328:                 self._update_attachment_failed(record_tip, attachment_tip, error_msg)
329:                 return False, retry_count, 0, error_msg
330: 
331:             # Write to temp file then rename
332:             with open(temp_path, 'wb') as f:
333:                 f.write(response.content)
334: 
335:             # Validate
336:             is_valid, file_size, validation_error = validate_attachment_file(temp_path, self.min_file_size)
337: 
338:             if not is_valid:
339:                 temp_path.unlink(missing_ok=True)
340:                 error_msg = f"Validation failed: {validation_error}"
341:                 self._update_attachment_failed(record_tip, attachment_tip, error_msg)
342:                 return False, retry_count, 0, error_msg
343: 
344:             # Rename temp to final
345:             temp_path.rename(output_path)
346: 
347:             # Calculate hash
348:             file_hash: str = calculate_md5_hash(output_path)
349: 
350:             download_end: datetime = datetime.now()
351:             download_duration: float = (download_end - download_start).total_seconds()
352: 
353:             # Update successful
354:             self.db_manager.execute_update(
355:                 """
356:                 UPDATE attachments SET
357:                     attachment_status = 'complete',
358:                     attachment_validation_status = 'valid',
359:                     file_size_bytes = %s,
360:                     file_hash_md5 = %s,
361:                     download_completed_at = %s,
362:                     download_duration_seconds = %s
363:                 WHERE record_tip = %s AND attachment_tip = %s
364:                 """,
365:                 (file_size, file_hash, download_end, download_duration, record_tip, attachment_tip)
366:             )
367: 
368:             file_size_mb: float = file_size / (1024 * 1024)
369:             logger.info(f"Downloaded attachment {attachment_sequence}: {filename} ({file_size_mb:.2f} MB)")
370: 
371:             return True, retry_count, file_size_mb, None
372: 
373:         except Exception as e:
374:             error_msg = f"Download exception: {e}"
375:             logger.error(f"Attachment download failed for {inspection_id}: {error_msg}")
376:             temp_path.unlink(missing_ok=True)
377:             self._update_attachment_failed(record_tip, attachment_tip, error_msg)
378:             return False, 0, 0, error_msg
379:     
380:     def _update_attachment_failed(self, record_tip: str, attachment_tip: str, error_msg: str) -> None:
381:         """Update attachment record as failed"""
382:         try:
383:             self.db_manager.execute_update(
384:                 """
385:                 UPDATE attachments SET
386:                     attachment_status = 'failed',
387:                     attachment_validation_status = 'validation_failed',
388:                     last_error_message = %s
389:                 WHERE record_tip = %s AND attachment_tip = %s
390:                 """,
391:                 (error_msg, record_tip, attachment_tip)
392:             )
393:         except Exception as e:
394:             logger.warning(f"Could not update attachment failure: {e}")
