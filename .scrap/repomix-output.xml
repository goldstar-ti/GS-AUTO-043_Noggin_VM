473: class RetryManager:
474:     """Manages retry logic and backoff calculations"""
475:     
476:     def __init__(self, config: 'ConfigLoader') -> None:
477:         self.max_retries: int = config.getint('retry', 'max_retry_attempts')
478:         self.backoff_multiplier: float = config.getfloat('retry', 'retry_backoff_multiplier')
479:         self.base_delay_minutes: int = config.getint('retry', 'base_retry_delay_minutes', fallback=5)
480:         self.max_delay_hours: int = config.getint('retry', 'max_retry_delay_hours', fallback=24)
481:     
482:     def calculate_next_retry_time(self, retry_count: int) -> datetime:
483:         """Calculate next retry time with exponential backoff"""
484:         if retry_count >= self.max_retries:
485:             # Far future = permanently failed
486:             return datetime.now() + timedelta(days=365 * 10)
487:         
488:         delay_minutes: float = self.base_delay_minutes * (self.backoff_multiplier ** retry_count)
489:         max_delay_minutes: float = self.max_delay_hours * 60
490:         delay_minutes = min(delay_minutes, max_delay_minutes)
491:         
492:         return datetime.now() + timedelta(minutes=delay_minutes)
493:     
494:     def should_retry(self, retry_count: int) -> bool:
495:         """Check if more retries are allowed"""
496:         return retry_count < self.max_retries
