120: def flatten_json(nested_json: Any, parent_key: str = '', sep: str = '_') -> Dict[str, Any]:
121:     """Flatten nested JSON into single-level dict with concatenated keys"""
122:     items: List[Tuple[str, Any]] = []
123:     
124:     if isinstance(nested_json, dict):
125:         for k, v in nested_json.items():
126:             new_key: str = f"{parent_key}{sep}{k}" if parent_key else k
127:             items.extend(flatten_json(v, new_key, sep).items())
128:     elif isinstance(nested_json, list):
129:         for i, v in enumerate(nested_json):
130:             new_key = f"{parent_key}{sep}{i}" if parent_key else str(i)
131:             items.extend(flatten_json(v, new_key, sep).items())
132:     else:
133:         items.append((parent_key, nested_json))
134:     
135:     return dict(items)
