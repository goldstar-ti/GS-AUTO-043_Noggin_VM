<file path="web/config_manager.py">
  1: import configparser
  2: import logging
  3: import os
  4: import re
  5: from typing import List, Dict, Optional
  6: 
  7: class ConfigManager:
  8:     def __init__(self, base_config_path: str = 'base_config.ini'):
  9:         self.base_config_path = base_config_path
 10:         self.base_config = configparser.ConfigParser()
 11:         self.logger = logging.getLogger(__name__)
 12:         self._load_base_config()
 13:         
 14:         # Mapping Object Types to their config files
 15:         # Assumes these files are in the current working directory or 'config/' folder
 16:         self.object_type_configs = {
 17:             'CCC': 'coupling_compliance_check_config.ini',
 18:             'FPI': 'forklift_prestart_inspection_config.ini',
 19:             'LCS': 'load_compliance_check_supervisor_manager_config.ini',
 20:             'LCD': 'load_compliance_check_driver_loader_config.ini',
 21:             'SO':  'site_observations_config.ini',
 22:             'TA':  'trailer_audits_config.ini'
 23:         }
 24: 
 25:     def _load_base_config(self):
 26:         """Loads the base configuration for DB and global settings."""
 27:         if not os.path.exists(self.base_config_path):
 28:             self.logger.error(f"Base configuration file not found at: {self.base_config_path}")
 29:             return
 30:         self.base_config.read(self.base_config_path)
 31: 
 32:     def get_db_config(self) -> Dict[str, str]:
 33:         """
 34:         Retrieves PostgreSQL credentials from base_config.ini.
 35:         """
 36:         db_config = {}
 37:         if self.base_config.has_section('postgresql'):
 38:             section = self.base_config['postgresql']
 39:             db_config['host'] = section.get('host', 'localhost')
 40:             db_config['port'] = section.get('port', '5432')
 41:             db_config['database'] = section.get('database', 'noggin_db')
 42:             db_config['user'] = section.get('user', 'noggin_app')
 43:             db_config['password'] = section.get('password', '')
 44:         return db_config
 45: 
 46:     def _get_specific_config(self, object_type: str) -> Optional[configparser.ConfigParser]:
 47:         """Helper to load the specific INI file for an object type."""
 48:         filename = self.object_type_configs.get(object_type)
 49:         if not filename:
 50:             self.logger.warning(f"No config file mapped for object type: {object_type}")
 51:             return None
 52: 
 53:         # Look in current directory or 'config' subdirectory
 54:         candidates = [filename, os.path.join('config', filename)]
 55:         for path in candidates:
 56:             if os.path.exists(path):
 57:                 config = configparser.ConfigParser()
 58:                 # strict=False allows for potential duplicate keys or loose syntax in templates
 59:                 config.read(path)
 60:                 return config
 61:         
 62:         self.logger.error(f"Config file {filename} for {object_type} not found.")
 63:         return None
 64: 
 65:     def get_inspection_type_label(self, object_type: str) -> str:
 66:         """
 67:         Retrieves the 'inspection_type' (e.g., 'Observation', 'Audit') from the config.
 68:         """
 69:         config = self._get_specific_config(object_type)
 70:         if config and config.has_section('object_type'):
 71:             return config.get('object_type', 'inspection_type', fallback=f"{object_type} Details")
 72:         return f"{object_type} Details"
 73: 
 74:     def _extract_label_from_template(self, content: str, keys_to_find: List[str]) -> str:
 75:         """
 76:         Searches the [template] content block to find the label preceding a tag.
 77:         Handles:
 78:         1. "Label: <tag>"
 79:         2. "Label:\n<tag>" (Multiline)
 80:         """
 81:         if not content:
 82:             return None
 83:         
 84:         for key in keys_to_find:
 85:             escaped_key = re.escape(key)
 86:             # Pattern 1: Inline "Label: <key>"
 87:             # ^\s* -> Start of line, optional whitespace
 88:             # ([^:\r\n<]+?) -> Capture group for label (non-greedy, no colons or newlines)
 89:             # \s*:\s* -> Colon separator
 90:             # .*? -> Any chars (like other tags)
 91:             # <key> -> The target tag
 92:             inline_pattern = r'(?im)^\s*([^:\r\n<]+?)\s*:\s*.*?<' + escaped_key + r'>'
 93:             
 94:             match = re.search(inline_pattern, content)
 95:             if match:
 96:                 return match.group(1).strip()
 97: 
 98:             # Pattern 2: Multiline "Label:\n<key>"
 99:             multiline_pattern = r'(?im)^\s*([^:\r\n<]+?)\s*:\s*\r?\n\s*<' + escaped_key + r'>'
100:             match_multi = re.search(multiline_pattern, content)
101:             if match_multi:
102:                 return match_multi.group(1).strip()
103:         
104:         return None
105: 
106:     def get_display_fields(self, object_type: str) -> List[Dict[str, str]]:
107:         """
108:         Returns a list of fields to display.
109:         1. Reads [fields] section to get API Key and DB Column.
110:         2. Parses [template] content to find the human-readable label.
111:         """
112:         fields_data = []
113:         config = self._get_specific_config(object_type)
114:         
115:         if not config:
116:             return []
117: 
118:         if not config.has_section('fields'):
119:             return []
120: 
121:         # Get the raw template content block
122:         template_content = ""
123:         if config.has_section('template') and config.has_option('template', 'content'):
124:             template_content = config.get('template', 'content')
125: 
126:         for api_key, value_str in config.items('fields'):
127:             # Parse value: "db_column:type" (e.g. "site_manager:string")
128:             parts = value_str.split(':')
129:             db_column = parts[0].strip()
130:             
131:             # We need to find the label.
132:             # The template might use the api_key OR the db_column name as the tag.
133:             label = self._extract_label_from_template(template_content, [api_key, db_column])
134:             
135:             # Fallback: If not found in template, Title Case the API key
136:             if not label:
137:                 # e.g. 'siteManager' -> 'Site Manager'
138:                 label = re.sub('([a-z0-9])([A-Z])', r'\1 \2', api_key).title()
139: 
140:             fields_data.append({
141:                 'label': label,
142:                 'db_column': db_column,
143:                 'api_key': api_key
144:             })
145:         
146:         return fields_data
</file>

<file path="web/email_manager.py">
 1: import os
 2: import zipfile
 3: import logging
 4: import io
 5: from email.message import EmailMessage
 6: from datetime import datetime
 7: from typing import List, Dict
 8: 
 9: class EmailManager:
10:     def __init__(self, temp_dir='/tmp/noggin_exports'):
11:         self.logger = logging.getLogger(__name__)
12:         self.temp_dir = temp_dir
13:         # Ensure temp directory exists
14:         os.makedirs(self.temp_dir, exist_ok=True)
15: 
16:     def generate_inspection_eml(self, inspection_data: Dict, attachments: List[Dict]) -> io.BytesIO:
17:         """
18:         Generates an EML file with a ZIP attachment containing the inspection files.
19:         """
20:         try:
21:             msg = EmailMessage()
22:             
23:             # Extract metadata
24:             insp_id = inspection_data.get('id', 'Unknown')
25:             insp_type = inspection_data.get('type_label', 'Inspection')
26:             
27:             # Headers
28:             msg['Subject'] = f"{insp_type} Export - {insp_id}"
29:             msg['From'] = "noreply@noggin-system.local"
30:             msg['To'] = "user@noggin-system.local" # Placeholder
31:             msg['Date'] = datetime.now()
32: 
33:             # Body
34:             body_content = f"""
35:             {insp_type} Export
36:             ----------------------------------------
37:             ID: {insp_id}
38:             Export Date: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}
39:             
40:             The original attachments for this record are included in the attached ZIP file.
41:             """
42:             msg.set_content(body_content)
43: 
44:             # Create ZIP in memory
45:             zip_buffer = io.BytesIO()
46:             with zipfile.ZipFile(zip_buffer, 'w', zipfile.ZIP_DEFLATED) as zip_file:
47:                 for att in attachments:
48:                     file_path = att.get('file_path')
49:                     filename = att.get('filename')
50:                     
51:                     if file_path and os.path.exists(file_path):
52:                         try:
53:                             # Add file to zip
54:                             zip_file.write(file_path, arcname=filename)
55:                         except Exception as zip_err:
56:                             self.logger.warning(f"Could not zip file {file_path}: {zip_err}")
57:                     else:
58:                         self.logger.warning(f"Attachment file missing: {file_path}")
59:             
60:             zip_buffer.seek(0)
61: 
62:             # Attach ZIP to Email
63:             msg.add_attachment(
64:                 zip_buffer.read(),
65:                 maintype='application',
66:                 subtype='zip',
67:                 filename=f"{insp_id}_attachments.zip"
68:             )
69: 
70:             # Return EML bytes
71:             eml_buffer = io.BytesIO(msg.as_bytes())
72:             eml_buffer.seek(0)
73:             return eml_buffer
74: 
75:         except Exception as e:
76:             self.logger.error(f"Error generating EML: {e}")
77:             raise
</file>

