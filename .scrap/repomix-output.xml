237:         <a href="{{ url_for('inspections') }}" class="{% if request.endpoint == 'inspections' %}active{% endif %}">Inspections</a>
238:         <a href="{{ url_for('hashes') }}" class="{% if request.endpoint == 'hashes' %}active{% endif %}">Hashes</a>
239:         <a href="{{ url_for('service_status') }}" class="{% if request.endpoint == 'service_status' %}active{% endif %}">Service Status</a>
