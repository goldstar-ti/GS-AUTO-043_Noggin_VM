 493: class BatchUpdater:
 494:     """
 495:     Handles batch updates of expected_inspection_id and expected_inspection_date.
 496: 
 497:     - Updates only where existing DB value is NULL
 498:     - Skips records with processing_status = 'complete'
 499:     - Inserts new records if TIP not found
 500:     """
 501: 
 502:     def __init__(self, db_manager: 'DatabaseConnectionManager', batch_size: int = 100) -> None:
 503:         self.db_manager = db_manager
 504:         self.batch_size = batch_size
 505:         self._pending: List[Dict[str, Any]] = []
 506:         self._updated_count: int = 0
 507:         self._inserted_count: int = 0
 508:         self._skipped_complete: int = 0
 509:         self._skipped_no_change: int = 0
 510: 
 511:     def add(self, record: Dict[str, Any]) -> None:
 512:         """Add a record to the batch"""
 513:         if record and record.get('tip'):
 514:             self._pending.append(record)
 515: 
 516:         if len(self._pending) >= self.batch_size:
 517:             self.flush()
 518: 
 519:     def flush(self) -> None:
 520:         """Flush pending records to database"""
 521:         if not self._pending:
 522:             return
 523: 
 524:         tips = [r['tip'] for r in self._pending]
 525:         existing_records = self._get_existing_records(tips)
 526:         # {tip: {processing_status, expected_inspection_id, expected_inspection_date}}
 527:         existing_map = {r['tip']: r for r in existing_records}
 528:         existing_tips = set(existing_map.keys())
 529: 
 530:         for record in self._pending:
 531:             tip = record['tip']
 532: 
 533:             if tip not in existing_tips:
 534:                 # TIP not in database - insert as new
 535:                 self._insert_new_record(record)
 536:                 continue
 537: 
 538:             db_record = existing_map[tip]
 539: 
 540:             if db_record['processing_status'] == 'complete':
 541:                 self._skipped_complete += 1
 542:                 continue
 543: 
 544:             # Determine which fields need updating (only if DB value is NULL)
 545:             updates = {}
 546:             csv_id = record.get('expected_inspection_id')
 547:             csv_date = record.get('expected_inspection_date')
 548: 
 549:             if csv_id and db_record['expected_inspection_id'] is None:
 550:                 updates['expected_inspection_id'] = csv_id
 551: 
 552:             if csv_date and db_record['expected_inspection_date'] is None:
 553:                 updates['expected_inspection_date'] = csv_date
 554: 
 555:             if not updates:
 556:                 self._skipped_no_change += 1
 557:                 continue
 558: 
 559:             self._update_record(tip, updates)
 560: 
 561:         self._pending.clear()
 562: 
 563:     def _get_existing_records(self, tips: List[str]) -> List[Dict[str, Any]]:
 564:         """Fetch existing records with relevant columns"""
 565:         if not tips:
 566:             return []
 567: 
 568:         placeholders = ','.join(['%s'] * len(tips))
 569:         query = f"""
 570:             SELECT tip, processing_status, expected_inspection_id, expected_inspection_date
 571:             FROM noggin_data
 572:             WHERE tip IN ({placeholders})
 573:         """
 574: 
 575:         try:
 576:             return self.db_manager.execute_query_dict(query, tuple(tips))
 577:         except Exception as e:
 578:             logger.error(f"Error fetching existing records: {e}")
 579:             return []
 580: 
 581:     def _insert_new_record(self, record: Dict[str, Any]) -> None:
 582:         """Insert a new record that doesn't exist in the database"""
 583:         current_time = datetime.now()
 584: 
 585:         columns = [
 586:             'tip',
 587:             'object_type',
 588:             'processing_status',
 589:             'csv_imported_at',
 590:             'created_at',
 591:             'updated_at',
 592:             'source_filename',
 593:             'expected_inspection_id',
 594:             'expected_inspection_date'
 595:         ]
 596: 
 597:         placeholders = ', '.join(['%s'] * len(columns))
 598: 
 599:         insert_sql = f"""
 600:             INSERT INTO noggin_data ({', '.join(columns)})
 601:             VALUES ({placeholders})
 602:             ON CONFLICT (tip) DO NOTHING
 603:         """
 604: 
 605:         values = (
 606:             record.get('tip'),
 607:             record.get('object_type'),
 608:             'csv_imported',
 609:             current_time,
 610:             current_time,
 611:             current_time,
 612:             record.get('source_filename'),
 613:             record.get('expected_inspection_id'),
 614:             record.get('expected_inspection_date')
 615:         )
 616: 
 617:         try:
 618:             result = self.db_manager.execute_update(insert_sql, values)
 619:             if result > 0:
 620:                 self._inserted_count += 1
 621:                 logger.info(f"Inserted new TIP during update: {record['tip'][:16]}...")
 622:         except Exception as e:
 623:             logger.error(f"Insert failed for new TIP {record.get('tip', 'unknown')[:16]}...: {e}")
 624: 
 625:     def _update_record(self, tip: str, updates: Dict[str, Any]) -> None:
 626:         """Update a single record with the specified fields"""
 627:         set_clauses = []
 628:         values = []
 629: 
 630:         for col, val in updates.items():
 631:             set_clauses.append(f"{col} = %s")
 632:             values.append(val)
 633: 
 634:         set_clauses.append("updated_at = %s")
 635:         values.append(datetime.now())
 636:         values.append(tip)
 637: 
 638:         update_sql = f"""
 639:             UPDATE noggin_data
 640:             SET {', '.join(set_clauses)}
 641:             WHERE tip = %s
 642:         """
 643: 
 644:         try:
 645:             result = self.db_manager.execute_update(update_sql, tuple(values))
 646:             if result > 0:
 647:                 self._updated_count += 1
 648:                 fields_updated = ', '.join(updates.keys())
 649:                 logger.debug(f"Updated {tip[:16]}...: {fields_updated}")
 650:         except Exception as e:
 651:             logger.error(f"Update failed for TIP {tip[:16]}...: {e}")
 652: 
 653:     def get_stats(self) -> Dict[str, int]:
 654:         """Get update statistics"""
 655:         return {
 656:             'updated': self._updated_count,
 657:             'inserted': self._inserted_count,
 658:             'skipped_complete': self._skipped_complete,
 659:             'skipped_no_change': self._skipped_no_change,
 660:             'pending': len(self._pending)
 661:         }
