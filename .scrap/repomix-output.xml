<file path="web/templates/dashboard.html">
  1: {% extends "base.html" %}
  2: 
  3: {% block title %}Dashboard - Noggin Processor{% endblock %}
  4: 
  5: {% block content %}
  6: <!-- 1. STATUS GRID -->
  7: <div class="stats-grid">
  8:     <!-- Transport Theme Config -->
  9:     {% set status_config = {
 10:         'complete': ('success', ''),
 11:         'pending': ('info', ''),
 12:         'failed': ('danger', ''),
 13:         'partial': ('warning', ''),
 14:         'api_error': ('danger', ''),
 15:         'csv_imported': ('secondary', ''),
 16:         'interrupted': ('warning', '')
 17:     } %}
 18:     
 19:     {% for status, count in stats.items() %}
 20:         {% set config = status_config.get(status, ('secondary', '')) %}
 21:         <div class="stat-card {{ config[0] }}">
 22:             <div class="label">{{ config[1] }} {{ status|upper }}</div>
 23:             <div class="number">{{ count }}</div>
 24:         </div>
 25:     {% endfor %}
 26: </div>
 27: 
 28: <!-- 2. OBJECT TYPE BREAKDOWN -->
 29: <div class="card" style="margin-top: 20px;">
 30:     <h2>Records by Object Type</h2>
 31:     <div style="display: flex; gap: 10px; flex-wrap: wrap;">
 32:         {% for item in type_stats %}
 33:             <div style="background: #f8f9fa; padding: 10px 15px; border-radius: 6px; border: 1px solid #ddd;">
 34:                 <span style="font-weight: bold; color: #555;">{{ item.object_type }}</span>
 35:                 <span style="background: var(--gs-gold); color: var(--gs-slate); padding: 2px 8px; border-radius: 10px; font-size: 0.9em; margin-left: 8px;">{{ item.count }}</span>
 36:             </div>
 37:         {% endfor %}
 38:     </div>
 39: </div>
 40: 
 41: <!-- 3. TODAY'S ACTIVITY -->
 42: <div class="card">
 43:     <h2>Today's Activity</h2>
 44:     <div class="stats-grid">
 45:         <div class="stat-card">
 46:             <div class="label">Total Processed</div>
 47:             <div class="number">{{ today_stats.total_today or 0 }}</div>
 48:         </div>
 49:         <div class="stat-card success">
 50:             <div class="label">Completed</div>
 51:             <div class="number">{{ today_stats.completed_today or 0 }}</div>
 52:         </div>
 53:         <div class="stat-card">
 54:             <div class="label">Success Rate</div>
 55:             <div class="number">
 56:                 {% if today_stats.total_today and today_stats.total_today > 0 %}
 57:                     {{ "%.1f"|format((today_stats.completed_today / today_stats.total_today) * 100) }}%
 58:                 {% else %}
 59:                     0%
 60:                 {% endif %}
 61:             </div>
 62:         </div>
 63:     </div>
 64: </div>
 65: 
 66: <!-- 4. RECENT ACTIVITY (With User, Flags & NO Processing Time) -->
 67: <div class="card">
 68:     <h2>Recent Activity</h2>
 69:     <table>
 70:         <thead>
 71:             <tr>
 72:                 <th>Reference / ID</th>
 73:                 <th>User / Team</th>
 74:                 <th>Date</th>
 75:                 <th>Status</th>
 76:                 <th>Attachments</th>
 77:                 <th>Updated</th>
 78:                 <th>Actions</th>
 79:             </tr>
 80:         </thead>
 81:         <tbody>
 82:             {% for item in recent %}
 83:             <tr>
 84:                 <!-- ID Column with Health Flags -->
 85:                 <td>
