 262: class CSVRowParser:
 263:     """Parses CSV rows and extracts preview fields with hash resolution."""
 264: 
 265:     def __init__(self, headers: List[str], preview_config: ObjectTypePreviewConfig,
 266:                  hash_resolver: HashResolver) -> None:
 267:         self.headers = headers
 268:         self.preview_config = preview_config
 269:         self.hash_resolver = hash_resolver
 270: 
 271:         self._column_indices: Dict[str, int] = {}
 272:         self._build_column_index_map()
 273: 
 274:     def _build_column_index_map(self) -> None:
 275:         """Build mapping of field names to column indices"""
 276:         for mapping in self.preview_config.preview_fields:
 277:             idx = find_column_index(self.headers, mapping.csv_column)
 278:             if idx >= 0:
 279:                 self._column_indices[mapping.csv_column] = idx
 280:             else:
 281:                 logger.debug(f"Column '{mapping.csv_column}' not found in CSV headers")
 282: 
 283:         idx = find_column_index(self.headers, self.preview_config.id_column)
 284:         if idx >= 0:
 285:             self._column_indices[self.preview_config.id_column] = idx
 286: 
 287:         idx = find_column_index(self.headers, self.preview_config.date_column)
 288:         if idx >= 0:
 289:             self._column_indices[self.preview_config.date_column] = idx
 290: 
 291:     def parse_row(self, row: List[str]) -> Dict[str, Any]:
 292:         """Parse a CSV row and extract all preview fields."""
 293:         row = [val.strip() for val in row]
 294: 
 295:         result: Dict[str, Any] = {}
 296: 
 297:         tip = row[0].strip() if row else ''
 298:         if not tip:
 299:             return {}
 300: 
 301:         result['tip'] = tip
 302:         result['object_type'] = self.preview_config.abbreviation
 303: 
 304:         date_col = self.preview_config.date_column
 305:         if date_col in self._column_indices:
 306:             idx = self._column_indices[date_col]
 307:             if idx < len(row):
 308:                 raw_date = row[idx].strip()
 309:                 result['expected_inspection_date'] = self._parse_date(raw_date)
 310: 
 311:         id_col = self.preview_config.id_column
 312:         if id_col in self._column_indices:
 313:             idx = self._column_indices[id_col]
 314:             if idx < len(row):
 315:                 raw_id = row[idx].strip()
 316:                 if raw_id:
 317:                     result['expected_inspection_id'] = raw_id
 318: 
 319:         for mapping in self.preview_config.preview_fields:
 320:             if mapping.csv_column not in self._column_indices:
 321:                 continue
 322: 
 323:             idx = self._column_indices[mapping.csv_column]
 324:             if idx >= len(row):
 325:                 continue
 326: 
 327:             raw_value = row[idx].strip()
 328:             if not raw_value:
 329:                 continue
 330: 
 331:             if mapping.is_hash_field:
 332:                 resolved, hash_val = self.hash_resolver.resolve_or_passthrough(raw_value)
 333: 
 334:                 if resolved:
 335:                     result[mapping.db_column] = resolved
 336:                 if hash_val and mapping.hash_db_column:
 337:                     result[mapping.hash_db_column] = hash_val
 338:             else:
 339:                 result[mapping.db_column] = raw_value
 340: 
 341:         return result
 342: 
 343:     def _parse_date(self, date_str: str) -> Optional[datetime]:
 344:         """Parse date string into datetime object"""
 345:         if not date_str:
 346:             return None
 347: 
 348:         formats = [
 349:             '%Y-%m-%dT%H:%M:%S.%fZ',
 350:             '%Y-%m-%dT%H:%M:%SZ',
 351:             '%Y-%m-%dT%H:%M:%S',
 352:             '%Y-%m-%d %H:%M:%S',
 353:             '%Y-%m-%d',
 354:             '%d-%b-%y',
 355:             '%d-%B-%Y',
 356:             '%d/%m/%Y',
 357:             '%m/%d/%Y',
 358:             '%d %b %Y',
 359:             '%d %b %y',
 360:             '%d-%b-%Y',
 361:             '%d-%b-%y',
 362:         ]
 363: 
 364:         for fmt in formats:
 365:             try:
 366:                 return datetime.strptime(date_str, fmt)
 367:             except ValueError:
 368:                 continue
 369: 
 370:         logger.debug(f"Could not parse date: {date_str}")
 371:         return None
