178: class APIClient:
179:     """Handles API requests with retry logic and circuit breaker integration"""
180:     
181:     def __init__(self, config: 'ConfigLoader', circuit_breaker: 'CircuitBreaker') -> None:
182:         self.config = config
183:         self.circuit_breaker = circuit_breaker
184:         
185:         self.base_url: str = config.get('api', 'base_url')
186:         self.attachment_base_url: str = config.get('api', 'media_service_url')
187:         self.headers: Dict[str, str] = config.get_api_headers()
188:         
189:         self.max_retries: int = config.getint('processing', 'max_api_retries')
190:         self.backoff_factor: int = config.getint('processing', 'api_backoff_factor')
191:         self.max_backoff: int = config.getint('processing', 'api_max_backoff')
192:         self.timeout: int = config.getint('processing', 'api_timeout')
193:         self.too_many_requests_sleep: int = config.getint('processing', 'too_many_requests_sleep_time')
194:     
195:     def make_request(self, url: str, tip_value: str) -> requests.Response:
196:         """Make API request with exponential backoff retry logic"""
197:         last_exception: Optional[Exception] = None
198: 
199:         for attempt in range(self.max_retries):
200:             try:
201:                 logger.debug(f"API request attempt {attempt + 1}/{self.max_retries} for TIP {tip_value}")
202:                 response: requests.Response = requests.get(url, headers=self.headers, timeout=self.timeout)
203:                 response._retry_count = attempt
204:                 logger.debug(f"Request attempt {attempt + 1} succeeded for TIP {tip_value}")
205:                 return response
206: 
207:             except requests.exceptions.ConnectionError as e:
208:                 last_exception = e
209:                 if attempt == self.max_retries - 1:
210:                     logger.error(f"All {self.max_retries} connection attempts failed for TIP {tip_value}")
211:                     raise
212:                 wait_time: float = min((self.backoff_factor ** attempt) * self.backoff_factor, self.max_backoff)
213:                 logger.warning(f"Connection failed for TIP {tip_value}, retrying in {wait_time}s (attempt {attempt + 1}/{self.max_retries})")
214:                 time.sleep(wait_time)
215: 
216:             except requests.exceptions.Timeout as e:
217:                 last_exception = e
218:                 if attempt == self.max_retries - 1:
219:                     logger.error(f"All {self.max_retries} timeout attempts failed for TIP {tip_value}")
220:                     raise
221:                 wait_time = min((self.backoff_factor ** attempt) * self.backoff_factor, self.max_backoff)
222:                 logger.warning(f"Request timeout for TIP {tip_value}, retrying in {wait_time}s (attempt {attempt + 1}/{self.max_retries})")
223:                 time.sleep(wait_time)
224: 
225:             except requests.exceptions.RequestException as e:
226:                 last_exception = e
227:                 if attempt == self.max_retries - 1:
228:                     logger.error(f"Request failed permanently for TIP {tip_value}: {e}")
229:                     raise
230:                 wait_time = self.backoff_factor
231:                 logger.warning(f"Request error for TIP {tip_value}, retrying in {wait_time}s (attempt {attempt + 1}/{self.max_retries})")
232:                 time.sleep(wait_time)
233: 
234:         if last_exception:
235:             raise last_exception
236:         raise Exception(f"Unexpected error in retry logic for TIP {tip_value}")
237:     
238:     def handle_error(self, response: requests.Response, tip_value: str, request_url: str) -> str:
239:         """Generate detailed error message from API response"""
240:         status_code: int = response.status_code
241: 
242:         try:
243:             response_text: str = response.text
244:             if response_text:
245:                 try:
246:                     error_json: Dict[str, Any] = response.json()
247:                     additional_info: str = f" Response body: {json.dumps(error_json, indent=2)}"
248:                 except json.JSONDecodeError:
249:                     additional_info = f" Response body: {response_text[:500]}{'...' if len(response_text) > 500 else ''}"
250:             else:
251:                 additional_info = " (No response body provided)"
252:         except Exception:
253:             additional_info = " (Could not read response body)"
254: 
255:         error_messages = {
256:             401: f"Authentication failed for TIP {tip_value}. Status code: {status_code} (Unauthorised). URL: {request_url}{additional_info}",
257:             403: f"Access forbidden for TIP {tip_value}. Status code: {status_code} (Forbidden). URL: {request_url}{additional_info}",
258:             404: f"Resource not found for TIP {tip_value}. Status code: {status_code} (Not Found). URL: {request_url}{additional_info}",
259:             429: f"Rate limit exceeded for TIP {tip_value}. Status code: {status_code} (Too Many Requests). URL: {request_url}{additional_info}",
260:         }
261: 
262:         if status_code in error_messages:
263:             return error_messages[status_code]
264:         elif 400 <= status_code < 500:
265:             return f"Client error for TIP {tip_value}. Status code: {status_code}. URL: {request_url}{additional_info}"
266:         elif 500 <= status_code < 600:
267:             return f"Server error for TIP {tip_value}. Status code: {status_code}. URL: {request_url}{additional_info}"
268:         else:
269:             return f"Unexpected response for TIP {tip_value}. Status code: {status_code}. URL: {request_url}{additional_info}"
