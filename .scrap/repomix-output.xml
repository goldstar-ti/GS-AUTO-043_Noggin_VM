 195: class PreviewFieldConfigLoader:
 196:     """
 197:     Loads preview field configurations from INI files.
 198: 
 199:     Each object type's INI file should have a [csv_import] section defining
 200:     which fields to extract for web search interface.
 201:     """
 202: 
 203:     HASH_COLUMN_MAP: Dict[str, str] = {
 204:         'team': 'team_hash',
 205:         'vehicle': 'vehicle_hash',
 206:         'trailer': 'trailer_hash',
 207:         'trailer2': 'trailer2_hash',
 208:         'trailer3': 'trailer3_hash',
 209:         'department': 'department_hash',
 210:     }
 211: 
 212:     def __init__(self, config_dir: Path) -> None:
 213:         self.config_dir = config_dir
 214:         self._configs: Dict[str, ObjectTypePreviewConfig] = {}
 215: 
 216:     def load_config(self, abbreviation: str) -> ObjectTypePreviewConfig:
 217:         """Load preview field config for an object type."""
 218:         if abbreviation in self._configs:
 219:             return self._configs[abbreviation]
 220: 
 221:         object_config = OBJECT_TYPES.get(abbreviation)
 222:         if not object_config:
 223:             raise CSVImportError(f"Unknown object type: {abbreviation}")
 224: 
 225:         config_file = self.config_dir / object_config.config_file
 226: 
 227:         preview_fields: List[PreviewFieldMapping] = []
 228: 
 229:         if config_file.exists():
 230:             parser = ConfigParser()
 231:             parser.read(config_file)
 232: 
 233:             if parser.has_section('csv_import'):
 234:                 for csv_col, db_col in parser.items('csv_import'):
 235:                     is_hash = db_col in self.HASH_COLUMN_MAP
 236:                     hash_col = self.HASH_COLUMN_MAP.get(db_col) if is_hash else None
 237: 
 238:                     preview_fields.append(PreviewFieldMapping(
 239:                         csv_column=csv_col,
 240:                         db_column=db_col,
 241:                         is_hash_field=is_hash,
 242:                         hash_db_column=hash_col
 243:                     ))
 244: 
 245:                 logger.debug(f"Loaded {len(preview_fields)} preview fields from {config_file.name}")
 246:             else:
 247:                 logger.warning(f"INI file {config_file.name} missing [csv_import] section. Only basic ID/Date will be mapped.")
 248:         else:
 249:             logger.warning(f"Config file not found: {config_file}. Only basic ID/Date will be mapped.")
 250: 
 251:         result = ObjectTypePreviewConfig(
 252:             abbreviation=abbreviation,
 253:             id_column=object_config.id_column,
 254:             date_column=object_config.date_column,
 255:             preview_fields=preview_fields
 256:         )
 257: 
 258:         self._configs[abbreviation] = result
 259:         return result
