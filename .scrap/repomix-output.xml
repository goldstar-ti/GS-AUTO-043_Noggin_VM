 49: @app.route('/inspection/<inspection_id>')
 50: def inspection_details(inspection_id):
 51:     """
 52:     Renders the enhanced inspection details page.
 53:     """
 54:     conn = None
 55:     try:
 56:         conn = get_db_connection()
 57:         cursor = conn.cursor(cursor_factory=RealDictCursor)
 58: 
 59:         # 1. Fetch Inspection Core Data
 60:         # We query unknown_hashes as the registry. 
 61:         cursor.execute("""
 62:             SELECT * FROM noggin_schema.unknown_hashes 
 63:             WHERE first_seen_inspection_id = %s 
 64:             LIMIT 1
 65:         """, (inspection_id,))
 66:         
 67:         record = cursor.fetchone()
 68:         
 69:         if not record:
 70:             logger.warning(f"Inspection {inspection_id} not found.")
 71:             # Basic fallback to infer type from ID (e.g. LCD-123 -> LCD)
 72:             object_type = inspection_id.split('-')[0] if '-' in inspection_id else 'Unknown'
 73:         else:
 74:             object_type = record.get('lookup_type', 'Unknown')
 75: 
 76:         # 2. Get Configuration
 77:         display_type_label = config_mgr.get_inspection_type_label(object_type)
 78:         display_fields = config_mgr.get_display_fields(object_type)
 79: 
 80:         # 3. Parse Data Payload
 81:         # We prioritize:
 82:         #   a) The 'resolved_value' JSON blob (if available)
 83:         #   b) Columns in the 'unknown_hashes' table
 84:         
 85:         data_payload = {}
 86:         if record and record.get('resolved_value'):
 87:             try:
 88:                 # Check if it looks like JSON
 89:                 val = record['resolved_value']
 90:                 if val and val.strip().startswith('{'):
 91:                     data_payload = json.loads(val)
 92:             except json.JSONDecodeError:
 93:                 pass
 94: 
 95:         # 4. Map Fields to Values
 96:         rendered_fields = []
 97:         for field in display_fields:
 98:             db_col = field['db_column']
 99:             # Try to find value in JSON payload first, then record column
100:             val = data_payload.get(db_col)
101:             if val is None and record:
102:                 val = record.get(db_col)
103:             
104:             # If still None, placeholder
105:             if val is None:
106:                 val = '-'
107:             
108:             # Formatting: If boolean, make it readable
109:             if isinstance(val, bool):
110:                 val = 'Yes' if val else 'No'
111: 
112:             rendered_fields.append({
113:                 'label': field['label'],
114:                 'value': val
115:             })
116: 
117:         # 5. Fetch Attachments
118:         cursor.execute("""
119:             SELECT record_tip, attachment_tip, filename, file_path 
120:             FROM noggin_schema.attachments 
121:             WHERE record_tip = (
122:                 SELECT tip_hash FROM noggin_schema.unknown_hashes 
123:                 WHERE first_seen_inspection_id = %s LIMIT 1
124:             )
125:         """, (inspection_id,))
126:         attachments = cursor.fetchall()
127: 
128:         return render_template(
129:             'inspection_details.html',
130:             inspection_id=inspection_id,
131:             type_label=display_type_label,
132:             fields=rendered_fields,
133:             attachments=attachments,
134:             object_type=object_type
135:         )
136: 
137:     except Exception as e:
138:         logger.error(f"Error serving inspection page: {e}", exc_info=True)
139:         return "Internal Server Error", 500
140:     finally:
141:         if conn: conn.close()
