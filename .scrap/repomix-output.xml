187:         )
188:         
189:         print("PostgreSQL Config:", config.get_postgresql_config())
190:         print("API Headers:", config.get_api_headers())
191:         print("Object Type Config:", config.get_object_type_config())
192:         
193:     except ConfigurationError as e:
194:         print(f"Configuration error: {e}")
