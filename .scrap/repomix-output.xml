 96:     def _build_context(self, response_data: Dict[str, Any], 
 97:                        inspection_id: str) -> Dict[str, Any]:
 98:         """Build context dictionary for template substitution"""
 99:         context: Dict[str, Any] = {
100:             'generation_date': datetime.now().strftime('%d-%m-%Y'),
101:             'full_name': self.full_name.upper(),
102:             'abbreviation': self.abbreviation,
103:             'inspection_type': self.inspection_type,
104:             'attachment_count': len(response_data.get('attachments', [])),
105:             'json_payload': json.dumps(response_data, indent=2, ensure_ascii=False),
106:             'show_json_payload_in_text_file': self.show_json,
107:             'show_all_fields': self.show_all_fields,
108:         }
109:         
110:         # Add all response fields directly
111:         for key, value in response_data.items():
112:             if key == '$meta':
113:                 continue
114:             
115:             # Format date fields
116:             if key in self.date_fields and value:
117:                 context[key] = self._format_date(value)
118:             else:
119:                 context[key] = value if value is not None else self.unknown_text
120:         
121:         # Process hash fields and add resolved values
122:         tip_value = response_data.get('$meta', {}).get('tip', '')
123:         
124:         for api_field, (db_column, field_type, hash_type) in self.field_mappings.items():
125:             value = response_data.get(api_field)
126:             
127:             if field_type == 'hash' and value and hash_type:
128:                 # Resolve hash to human-readable value
129:                 resolved = self.hash_manager.lookup_hash(
130:                     hash_type, str(value), tip_value, inspection_id
131:                 )
132:                 context[f"{api_field}_resolved"] = resolved
133:                 
134:                 # Also add without _resolved suffix for simpler templates
135:                 base_name = api_field.replace('_hash', '').replace('Hash', '')
136:                 context[f"{base_name}_resolved"] = resolved
137:         
138:         return context
